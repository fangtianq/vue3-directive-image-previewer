{
  "name": "vue3-directive-image-previewer",
  "private": false,
  "version": "2.0.1",
  "description": "",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/exports.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js",
      "types": "./dist/exports.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build-js": "vite build",
    "test:ui": "vitest --ui",
    "test:update": "vitest -u",
    "prepublishOnly": "npm run build",
    "play": "pnpm -C examples/vite run dev",
    "play:build": "pnpm -C examples/vite run build",
    "build-types": "vue-tsc -p tsconfig.build.json",
    "play:preview": "pnpm -C examples/vite run preview",
    "build": "npm-run-all --parallel build-js build-types",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fangtianq/vue3-directive-image-previewer.git"
  },
  "keywords": [
    "vue3",
    "preview",
    "image",
    "directive"
  ],
  "license": "MIT",
  "author": {
    "name": "fangtianq",
    "email": "506073383@qq.com",
    "url": "https://github.com/fangtianq"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.25.8",
    "bumpp": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4",
    "unocss": "^0.47.6",
    "vite": "^4.0.1",
    "vitest": "^0.25.8",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.13",
    "less": "^4.1.3"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  }
}
